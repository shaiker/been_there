<%= form_for @image, :html => {:multipart => true} do |f| %>
  <p>
    <label>Upload Image:</label>
    <%= image_tag(@image.url) if @image.url? %>
    <%= f.file_field :url %></br>
    caption: <%= f.text_field :caption %></br>
    user_id: <%= f.text_field :user_id %></br>
    <%= f.submit %>
  </p>
<% end %>
